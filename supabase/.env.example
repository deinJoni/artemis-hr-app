# Values below mirror the root .env.example but stay scoped to Supabase CLI usage.
# Replace the placeholders with the credentials printed from `supabase start` or
# the keys from your hosted Supabase project.
SUPABASE_URL=https://<project>.supabase.co
SUPABASE_ANON_KEY=sb_publishable_xyz
SUPABASE_SERVICE_ROLE_KEY=sb_secret_xyz

# Optional: required when running `supabase link` against a hosted project
SUPABASE_PROJECT_ID=<project-ref>
SUPABASE_ACCESS_TOKEN=

# Local database password (the CLI defaults to "postgres")
SUPABASE_DB_PASSWORD=postgres
